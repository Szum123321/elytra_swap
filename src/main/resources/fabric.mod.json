{
  "schemaVersion": 1,
  "id": "elytra_swap",
  "version": "${version}",

  "name": "Elytra Swap",
  "description": "This mod automatically swaps your main armour with elytra when falling",
  "authors": [
    "Szum123321"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "GPL-3",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.szum123321.elytra_swap.ElytraSwap"
    ],
    "client": [
      "net.szum123321.elytra_swap.client.ElytraSwapClientInit"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "minecraft": "1.15.x",
    "cotton-config": "*"
  },
  "suggests": {
    "flamingo": "*"
  }
}
